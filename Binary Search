#include<iostream>
#include<cmath>
using namespace std;

int main() {
    int arr[6] = {2, 4, 9, 15, 19, 20};
    int key = 19;
    int left=0;
    int right= 5;
    while (left <= right) {
        int m = (left + right)/ 2;
        if (arr[m] == key){
            cout<<"Element found at "<<m;
            return m;}
        else if (arr[m] < key)
            left = m + 1; 
        else if (arr)
            right = m - 1; 
    }
    cout<<"Element is not present!!"<<endl;
}
